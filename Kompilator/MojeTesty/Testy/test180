VAR
	n
BEGIN
	n := 5;
	FOR i FROM 10 DOWNTO 1 DO
		n := n + 1;
		FOR j FROM n DOWNTO 5 DO
			WRITE j;
		ENDFOR 
		FOR j FROM 10 DOWNTO n DO
			WRITE j;
		ENDFOR
	ENDFOR
END

